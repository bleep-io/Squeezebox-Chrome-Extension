{
  "name": "SqueezeControl",
  "version": "1.0",
  "options_page": "options.html",
  "background": "background.js",
  "manifest_version": 2,
  "description": "An extension to control your squeezebox player from Google Chrome.",
   "permissions": [
    "storage",
    "tabs",
    "<all_urls>"
  ],

  "browser_action": {
    "default_icon": "icon.png",
	"default_popup": "popup.html"
  }

}